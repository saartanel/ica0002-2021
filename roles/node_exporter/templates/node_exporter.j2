server {
	listen 80 default_server;
    root /var/www/html;
	location / {
		uwsgi_pass localhost:5000;
		include uwsgi_params;
		try_files $uri $uri/ =404;
	}
    location /metrics {
		proxy_pass http://127.0.0.1:9100/metrics;
	}
    location /prometheus {
        proxy_pass http://{{ prometheus_host }}.{{ domain }}:9090;
    }


}
